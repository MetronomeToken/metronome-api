{
  "server": {
    "maxResultsLimit": "SERVER_MAX_RESULTS_LIMIT",
    "port": "SERVER_PORT"
  },

  "newrelic": {
    "licenseKey": "NEW_RELIC_LICENSE_KEY",
    "logginGgLevel": "NEW_RELIC_LOGGING_LEVEL"
  },

  "cors": {
    "origin": "*"
  },

  "mongo": {
    "url": "MONGO_URL",
    "testUrl": "MONGO_TEST_URL"
  },

  "logger": {
    "sentry": {
      "dsn": "LOGGER_SENTRY_DSN"
    },

    "console": {
      "level": "LOGGER_CONSOLE_LEVEL",
      "timestamp": true,
      "handleExceptions": true,
      "humanReadableUnhandledException": true,
      "colorize": true
    },

    "papertrail": {
      "colorize": true,
      "flushOnClose": true,
      "handleExceptions": true,
      "host": "LOGGER_PAPERTRAIL_HOST",
      "inlineMeta": true,
      "level": "LOGGER_PAPERTRAIL_LEVEL",
      "port": "LOGGER_PAPERTRAIL_PORT",
      "program": "LOGGER_PAPERTRAIL_PROGRAM"
    }
  },

  "eth": {
    "enabled": true,
    "ipcPath": "ETH_IPC_PATH",
    "webSocketUrl": "ETH_WS_API_URL",
    "tokenAddress": "ETH_TOKEN_ADDR",
    "auctionAddress": "ETH_AUCT_ADDR",
    "minterAddress": "ETH_MINTER_ADDR",
    "exportStartBlock": "ETH_EXPORT_START_BLOCK"
  }
}
