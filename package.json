{
  "name": "metronome-api",
  "version": "1.0.0",
  "description": "ðŸ–² Metronome Token REST API",
  "keywords": [
    "met",
    "token",
    "metronome",
    "api",
    "rest"
  ],
  "homepage": "https://github.com/autonomoussoftware/metronome-api#readme",
  "bugs": {
    "url": "https://github.com/autonomoussoftware/metronome-api/issues"
  },
  "license": "MIT",
  "author": "Ignacio Anaya <ignacio.anaya89@gmail.com>",
  "repository": "autonomoussoftware/metronome-api",
  "scripts": {
    "check": "dependency-check . && dependency-check . --unused --no-dev",
    "check:outdated": "npm-check -i eslint-config* -i husky",
    "dev": "nodemon server",
    "debug": "node --inspect ./bin/met-api",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --fix .js",
    "test": "jest --forceExit -i",
    "test:dev": "jest --watch",
    "prepush": "npm test",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "async": "2.6.1",
    "before-exit": "1.0.0",
    "body-parser": "1.18.3",
    "config": "1.30.0",
    "cors": "2.8.4",
    "express": "4.16.3",
    "express-rest-api": "0.1.0",
    "figlet": "1.2.0",
    "metronome-contracts": "1.4.0",
    "mongoose": "5.1.2",
    "mongoose-count-and-find": "1.0.0",
    "mongoose-cu-timestamps": "1.0.0",
    "newrelic": "4.1.2",
    "on-finished": "2.3.0",
    "pretty-ms": "3.2.0",
    "promise-all-props": "1.0.1",
    "request": "2.87.0",
    "socket.io": "2.1.1",
    "time-bombs": "1.0.0",
    "web3": "1.0.0-beta.34",
    "winston": "2.4.2",
    "winston-child-logger": "0.0.1",
    "winston-papertrail": "1.0.5",
    "winston-sentry-transport": "1.0.0"
  },
  "devDependencies": {
    "dependency-check": "3.1.0",
    "eslint": "4.19.1",
    "eslint-config-bloq": "1.3.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsdoc": "3.7.1",
    "eslint-plugin-mocha": "5.0.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-prefer-arrow": "1.1.2",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "husky": "0.14.3",
    "jest": "23.0.1",
    "nodemon": "1.17.5",
    "npm-check": "5.7.1"
  },
  "engines": {
    "node": "8",
    "npm": "5"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    },
    "roots": [
      "test/"
    ]
  }
}
